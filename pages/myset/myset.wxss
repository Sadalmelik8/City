.title {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 35rpx;
  font-weight: bold;
  border-bottom: 0.5rpx #ecebeb solid;
  padding: 0 24rpx;
  background: #fff;
  margin-bottom: 10rpx;
}

.res {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  font-weight: bold;
  border-bottom: 0.5rpx #ecebeb solid;
  background: #fff;
  padding: 0 24rpx;
}
.logoff{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 30rpx;
  font-weight: bold;
  border-bottom: 0.5rpx #ecebeb solid;
  background: #fff;
  margin-top: 20rpx;
  text-align: center;
}
.right {
  float: right;
}
